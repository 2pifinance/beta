{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "next",
    "next/core-web-vitals",
    "plugin:react/recommended"
  ],
  "globals": {
    "process": "readonly",
    "require": "readonly"
  },
  "overrides": [
    {
      "env": {
        "jest": true
      },
      "files": [
        "**/*.test.js"
      ],
      "plugins": [
        "jest"
      ]
    }
  ],
  "rules": {
    "eqeqeq": "error",
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_"
      }
    ],
    "no-undef": "warn",
    "react/react-in-jsx-scope": "off"
  }
}
